{
  "name": "podcaster-react",
  "version": "0.0.1",
  "description": "React app for exploring most popular podcasts from iTunes",
  "main": "index.ts",
  "scripts": {
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "start": "webpack serve --config ./webpack.config.dev.js",
    "build": "webpack build --config ./webpack.config.prod.js",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx} --config ./.prettierrc",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "podcast"
  ],
  "author": "Adrian Munoz",
  "license": "ISC",
  "devDependencies": {
